@using Projecore.Models
@model List<Uyeler>
    
    @{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
    }
    <br />
    
    <table id="dtDynamicVerticalScrollExample" class="table table-striped table-bordered table-sm" cellspacing="0"
  width="100%">
        <thead>
       <tr>
           <th>ID</th>
           <th>Tc Kimlik</th>
           <th>Ad</th>
           <th>Soyad</th>
           <th>Doğum Yılı</th>
           <th>Telefon</th>
           <th>Mail</th>
           <th>Firma Adı</th> 
       </tr>
       </thead>
       <tbody>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.id</td>
            <td>@x.tc</td>
            <td>@x.ad</td>
            <td>@x.soyad</td>
            <td>@x.dogumyil</td>
            <td>@x.telefon</td>
            <td>@x.mail</td>
            <td>@x.Firma.firma_adi</td>
        </tr>
      
    }
    </tbody>
    </table>
    <br /><br />
    <a href="/Uyeler/YeniUye"  class="btn btn-primary">Yeni Üye</a>